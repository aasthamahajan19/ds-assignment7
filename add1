#include <iostream>
using namespace std;

int main(){
    int n;
    cout << "Enter size: ";
    cin >> n;

    int a[n];
    int maxVal = 0;

    cout << "Enter elements: ";
    for(int i = 0; i < n; i++){
        cin >> a[i];
        maxVal = max(maxVal, a[i]);
    }

    int count[maxVal+1] = {0};
    
    for(int i=0; i<n; i++) count[a[i]]++;

    cout << "Sorted: ";
    for(int i=0; i<=maxVal; i++){
        while(count[i]--){
            cout << i << " ";
        }
    }
}
